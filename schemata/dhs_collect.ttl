@prefix dcat:    <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dctype:  <http://purl.org/dc/dcmitype/> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix prov:    <http://www.w3.org/ns/prov#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdo:     <http://schema.org/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard:   <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix dhs:     <https://usdhs.github.io/dcat-tool/#> .
@prefix dt:      <http://github.com/usdhs/dcat-tool/0.1#> .
@prefix sh:      <http://www.w3.org/ns/shacl#> .
@prefix usg:     <http://resources.data.gov/resources/dcat-us/#> .

dhs:DataInventoryRecord
    a owl:Class ;
    rdfs:subClassOf dcat:Dataset ;
    .

dhs:Characteristics
    a owl:Class ;
    rdfs:subClassOf dcat:Dataset ;
    .

dhs:characteristicsShape
    a sh:NodeShape ;
    sh:targetClass dhs:Characteristics ;
    rdfs:comment "The characteristics of a dataset - i.e. whether it contains PII, PMI, biometrics, etc."@en ;
    sh:property
    [ sh:path dhs:ch-person-level        ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-financial           ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-event-records       ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-faces               ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-fingerprints        ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-cui                 ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-phi                 ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-pii                 ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-geospatial          ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-environmental       ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-fisa                ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-8usc1367            ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-propin              ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-immigration         ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-criticalInfrastructure; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-pcii               ;  sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-biometrics         ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ],
    [ sh:path dhs:ch-disseminationRestrictions; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:security ];
    sh:closed true;
    sh:ignoredProperties ( rdf:type )
    .

dhs:dataInventoryRecordShape
    a sh:NodeShape ;
    sh:targetClass dhs:DataInventoryRecord ;
    rdfs:comment "Specify the DCATv3 fields (and other fields) that are being collected for the DHS Data Inventory Project."@en ;
    owl:versionInfo "1.5" ;
    dcterms:modified "2022-02-17"^^xsd:date ;
    sh:property
    [ sh:path dcterms:identifier         ; sh:minCount 1 ; sh:maxCount 1 ; sh:maxLength 128 ; sh:pattern "^\\S*$" ; sh:datatype xsd:string ; sh:message "Identifier (string) is required and must not be more than 100 characters." ; dt:title "unique identifier"; rdfs:comment "<p>Similar to a serial number associated with your laptop, the identifier is a unique and specific combination of characters that identify the resource. The combination must start with the DHS component code, is limited to 128 characters, and be in the following format: code-xxx-x</p><p>Example:<ul class='no-bullets'><li>mgmt-test-2</li><li>fema-1234-D</li></ul></p>"@en; a sh:PropertyShape ; dt:group dt:highLevel; ],

    [ sh:path dcterms:title              ; sh:minCount 1 ; sh:maxCount 1 ; sh:maxLength 256 ; sh:datatype xsd:string ; sh:message "Title (string) is required and must not be more than 255 characters." ; rdfs:comment "<p>The official name (title) given to the resource, limited to 255 characters. Acronyms should be used sparingly and only when explained in the description.</p><p>Example:<ul class='no-bullets'><li>DHS Data Inventory Introduction</li></ul></p>"@en; dhs:excelWidth 30; a sh:PropertyShape ; dt:group dt:highLevel; ],

    [ sh:path dcterms:description        ; sh:minCount 1 ; sh:maxCount 1 ; sh:maxLength 4000 ; sh:datatype xsd:string ; sh:message "Description (string) is required and must not be more than 4,000 characters." ; a sh:PropertyShape ; dt:group dt:highLevel; ],

    [ sh:path dcat:keyword               ; sh:minCount 0 ; sh:datatype xsd:string ; a sh:PropertyShape ; dt:group dt:highLevel; ],

    [ sh:path dcterms:publisher          ; sh:minCount 0 ; sh:or ([ sh:class sdo:Person ][ sh:class vcard:Individual ][ sh:class sdo:Organization ][ sh:class vcard:Organization ][ sh:datatype xsd:string]) ; a sh:PropertyShape ; dt:group dt:highLevel; ],

    [ sh:path usg:accessLevel            ; sh:minCount 1 ; sh:maxCount 1 ; sh:in("public" "restricted public" "non-public") ; sh:message "Must be one of the following (exactly): 'public', 'restricted public' or 'non-public'" ; a sh:PropertyShape ; dt:group dt:highLevel; ],

    [ sh:path dhs:dataCatalogRecordAccessLevel ; sh:minCount 1 ; sh:maxCount 1 ; sh:in("public" "non-public"); sh:message "Must be one of the following (exactly): 'public' or 'non-public'"  ; a sh:PropertyShape ; dt:group dt:highLevel; ],

    [ sh:path dcterms:issued             ; sh:minCount 1 ; sh:maxCount 1 ; sh:datatype xsd:date; sh:message "Issued date is required and must be a date (like 12/31/2021)." ; a sh:PropertyShape ; dt:group dt:highLevel; ],

    [ sh:path dhs:component              ; sh:minCount 1 ;  a sh:PropertyShape ; dt:group dt:highLevel ],


    [ sh:path dcterms:creator            ; sh:minCount 0 ; sh:or ([ sh:class sdo:Person ][ sh:class vcard:Individual ][ sh:class sdo:Organization ][ sh:class vcard:Organization ][ sh:datatype xsd:string]) ; a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:governance             ; sh:minCount 0 ; sh:or ([ sh:class sdo:Person ][ sh:class vcard:Individual ][ sh:class sdo:Organization ][ sh:class vcard:Organization ][ sh:datatype xsd:string]) ; a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:owner                  ; sh:minCount 0 ; sh:or ([ sh:class sdo:Person ][ sh:class vcard:Individual ][ sh:datatype xsd:string]) ; a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:steward                ; sh:minCount 0 ; sh:or ([ sh:class sdo:Person ][ sh:class vcard:Individual ][ sh:datatype xsd:string]) ; a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:custodian              ; sh:minCount 0 ; sh:or ([ sh:class sdo:Person ][ sh:class vcard:Individual ][ sh:datatype xsd:string]) ; a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path usg:primaryITInvestmentUII ; sh:minCount 0 ; sh:datatype xsd:string ; sh:message "UII must be a string, typically of the format NNN-NNNNNNNNN and can have multiple, comma separated values." ;  a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:fismaID                ; sh:minCount 0 ; sh:datatype xsd:string ; a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dcterms:references         ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:sharingAgreements      ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dcat:contactPoint          ; sh:minCount 0 ; sh:or ([ sh:class sdo:Person ][ sh:class vcard:Individual ][ sh:datatype xsd:string]) ; a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:collectionAuthority    ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:retentionAuthority     ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:releaseAuthority       ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path dhs:recordsSchedule        ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],
    [ sh:path usg:systemOfRecords        ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:dataGovernanceInformation;],

    [ sh:path dcterms:conformsTo         ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:standards ],
    [ sh:path dhs:conformsFIPS           ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:standards ],
    [ sh:path dhs:conformsNIEMPercent    ; sh:minCount 0 ; sh:maxCount 1 ; sh:datatype xsd:integer ; sh:message "Must be a integer between 0 and 100" ; sh:minInclusive 0 ; sh:maxInclusive 100 ; sh:message "Must be an integer between 0 and 100." ; a sh:PropertyShape ; dt:group dt:standards ],
    [ sh:path dhs:conformsUnicode        ; sh:minCount 0 ; sh:maxCount 1 ; sh:datatype xsd:boolean ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:standards ],
    [ sh:path dhs:identitiesNativeScript ; sh:minCount 0 ; sh:maxCount 1 ; sh:datatype xsd:boolean ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:standards ],
    [ sh:path dhs:transliterationStandard; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:standards ],

    [ sh:path dhs:sourceDatasets         ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:provenance; ],
    [ sh:path dhs:destinationDatasets    ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:provenance; ],
    [ sh:path usg:describedBy            ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:provenance; ],
    [ sh:path usg:describedByType        ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:provenance; ],
    [ sh:path dcterms:isPartOf           ; sh:minCount 0 ; sh:maxLength 5000 ; a sh:PropertyShape ; dt:group dt:provenance; ],

    [ sh:path dcterms:accrualPeriodicity     ; sh:minCount 0 ; sh:datatype xsd:duration ;  a sh:PropertyShape ; dt:group dt:dataset ],

    [ sh:path dcterms:spatial            ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:datasetGeospatial ],
    [ sh:path dcat:spatialResolutionInMeters  ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:datasetGeospatial],

    [ sh:path dcterms:temporal           ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:datasetTemporal],
    [ sh:path dcat:temporalResolution    ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:datasetTemporal],

    [ sh:path dhs:dataQualityKnown       ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:dataQuality ],
    [ sh:path dhs:dataQualityPercent     ; sh:minCount 0 ; sh:maxCount 1 ; sh:datatype xsd:integer ; sh:message "Must be a integer between 0 and 100" ;   a sh:PropertyShape ; dt:group dt:dataQuality ],
    [ sh:path dhs:dataQualityAssessment  ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:dataQuality ],
    [ sh:path usg:dataQuality            ; sh:minCount 0 ; sh:datatype xsd:boolean ; sh:maxCount 1 ; sh:message "Use either 'true' or 'false' with lower case" ; a sh:PropertyShape ; dt:group dt:dataQuality ],

    [ sh:path dcterms:format             ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:distribution ],
    [ sh:path dcat:accessURL             ; sh:minCount 0 ;  sh:maxCount 1 ; sh:datatype xsd:anyURI; a sh:PropertyShape ; dt:group dt:distribution ],
    [ sh:path dhs:accessURLNIEM          ; sh:minCount 0 ;  sh:maxCount 1 ; sh:datatype xsd:anyURI; a sh:PropertyShape ; dt:group dt:distribution ],
    [ sh:path usg:modified               ; sh:minCount 0 ; sh:maxCount 1 ; sh:datatype xsd:date; a sh:PropertyShape ; dt:group dt:distribution ],
    [ sh:path dhs:vendor                 ; sh:minCount 0 ; sh:or ([ sh:class sdo:Person ][ sh:class vcard:Individual ][ sh:class sdo:Organization ][ sh:class vcard:Organization ][ sh:datatype xsd:string]) ;  a sh:PropertyShape ; dt:group dt:distribution ],
    [ sh:path dcterms:license            ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:distribution ],
    [ sh:path dcterms:accessRights       ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:distribution ],
    [ sh:path dcat:theme                 ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:distribution ],
    [ sh:path dhs:functionalDataDomain   ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:distribution ],
    [ sh:path dcat:mediaType             ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:distribution ],

    [ sh:path dhs:tableCount             ; sh:minCount 0 ; sh:maxCount 1 ; sh:datatype xsd:integer ; sh:minInclusive 0 ; sh:message "Only one value is allowed and must be a non-negative integer" ;  a sh:PropertyShape ; dt:group dt:size ],
    [ sh:path dhs:recordCount            ; sh:minCount 0 ; sh:maxCount 1 ; sh:datatype xsd:integer ; sh:minInclusive 0 ; sh:message "Only one value is allowed and must be a non-negative integer" ;  a sh:PropertyShape ; dt:group dt:size ],
    [ sh:path dcat:byteSize              ; sh:minCount 0 ; sh:maxCount 1 ; sh:datatype xsd:integer ; sh:minInclusive 0 ; sh:message "Only one value is allowed and must be a non-negative integer" ; a sh:PropertyShape ; dt:group dt:size ],

    [ sh:path dhs:datasetClassification  ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:security ],

    [ sh:path dhs:hostingLocation        ; sh:minCount 0 ;  a sh:PropertyShape ; dt:group dt:location ],
    [ sh:path dhs:hostedInCloud          ; sh:minCount 0 ; sh:datatype xsd:boolean ; a sh:PropertyShape ; dt:group dt:location ],
    [ sh:path dhs:easilyAccessibleByCreatingComponent  ; sh:minCount 0 ; sh:datatype xsd:boolean ;  a sh:PropertyShape ; dt:group dt:location ],
    [ sh:path dhs:easilyAccessibleByAllComponents  ; sh:minCount 0 ; sh:datatype xsd:boolean ; a sh:PropertyShape ; dt:group dt:location ],
    [ sh:path dhs:easilyAccessibleByGeneralPublic  ; sh:minCount 0 ; sh:datatype xsd:boolean ; a sh:PropertyShape ; dt:group dt:location ],

    [ sh:path usg:encryptionAlgorithm    ; sh:minCount 0 ; sh:datatype xsd:string                   ; a sh:PropertyShape; dt:group dt:publication ],
    [ sh:path dhs:recordTransmission     ; sh:minCount 0 ; sh:datatype xsd:date ; sh:maxCount 1 ;  a sh:PropertyShape ; dt:group dt:publication ],
    [ sh:path dhs:validityTime           ; sh:minCount 0 ; sh:datatype xsd:duration ; sh:maxCount 1 ;  a sh:PropertyShape ; dt:group dt:publication ],
    [ sh:path dhs:characteristics                  ; sh:minCount 0 ; sh:class dhs:Characteristics; a sh:PropertyShape ];
    sh:closed true;
    skos:definition "Constraints for a record in a data catalog, describing the registration of a single dataset or data service."@en ;
    sh:ignoredProperties ( rdf:type )
    .
